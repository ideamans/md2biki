    "unist-util-visit": "^5.0.0",
    "commander": "^11.1.0",
    "glob": "^10.3.10",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}