{
  "name": "md2biki",
  "version": "1.0.0",
  "description": "Convert Markdown documents to Backlog Wiki format",
  "main": "dist/index.js",
  "bin": {
    "md2biki": "./dist/cli.js"
  },
  "scripts": {
